SHELL=/bin/sh

RANLIB=@RANLIB@

MAKE=make
LIBDIR=../lib
YAZLIBS=$(LIBDIR)/libodr.a $(LIBDIR)/libasn.a $(LIBDIR)/libcomstack.a \
	$(LIBDIR)/libserver.a $(LIBDIR)/libutil.a $(LIBDIR)/libret.a
LIB=$(LIBDIR)/libyaz.a

all: $(LIB)

$(LIB): $(LIBS)
	rm -f $(LIB)
	for i in $(YAZLIBS); do echo $$i; ar x $$i; rm -f __*; done
	ar qc $(LIB) *.o
	rm -f *.o
	$(RANLIB) $(LIB)

clean:
	-rm -f *.o

depend:
