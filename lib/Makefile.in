SHELL=/bin/sh

RANLIB=@RANLIB@

MAKE=make
YAZLIBS=libodr.a libasn.a libcomstack.a \
	libserver.a libutil.a libret.a
LIB=libyaz.a

all: $(LIB)

$(LIB): $(LIBS)
	rm -f $(LIB)
	for i in $(YAZLIBS); do echo $$i; ar x $$i; rm -f __*; done
	ar qc $(LIB) *.o
	rm -f *.o
	$(RANLIB) $(LIB)

clean:
	-rm -f *.o $(LIB)

depend:
