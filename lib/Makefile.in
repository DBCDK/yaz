SHELL=/bin/sh

RANLIB=@RANLIB@

MAKE=make
YAZLIBS=libodr.a libasn.a libcomstack.a libserver.a \
	libzutil.a libutil.a libccl.a libret.a @ILLLIB@
LIB=libyaz.a

all: $(LIB)

$(LIB): $(YAZLIBS)
	rm -f $(LIB)
	for i in $(YAZLIBS); do echo $$i; ar x $$i; rm -f __*; done
	ar qc $(LIB) *.o
	rm -f *.o
	$(RANLIB) $(LIB)

clean:
	-rm -f *.[oa]

depend:
